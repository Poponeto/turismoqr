<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="TurismoQR.ObjetosNegocio.Usuarios.Permisos.PermisoUsuario" table="permisoUsuario">
      <id column="idPermisoUsuario" name="idObjeto">
            <generator class="guid"/>
        </id>
         <many-to-one name="permiso" class="TurismoQR.ObjetosNegocio.Usuarios.Permisos.Permiso" cascade="all" foreign-key="idPermiso"/>
      </class>
</hibernate-mapping>
