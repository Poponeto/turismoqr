<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <joined-subclass name="TurismoQR.ObjetosNegocio.Informacion.Imagen" table="imagen" extends="TurismoQR.ObjetosNegocio.Recurso.Recurso">
        <key column="idImagen"/>  
        <property name="extension"/>
        <many-to-one name="informacion" class="TurismoQR.ObjetosNegocio.Informacion.Informacion" cascade="all" foreign-key="idInformacion" />
    </joined-subclass>
</hibernate-mapping>
 