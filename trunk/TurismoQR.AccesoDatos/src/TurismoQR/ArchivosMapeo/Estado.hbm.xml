<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="TurismoQR.ObjetosNegocio.Estados.Estado" table="estado">
        <id name="idObjeto" column="idEstado">
            <generator class="guid"> 
            </generator>
        </id>
        <property name="nombreDeEstado"/>
        <property name="fechaInicioPeriodo" type="timestamp" />
        <property name="fechaFinPeriodo" type="timestamp"/>
    </class>
</hibernate-mapping>
